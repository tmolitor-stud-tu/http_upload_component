[xmpp]
max_file_size = 134217730										;128MiB maximum file size (in bytes)
allowed_domain = "example.org"									;allowed domain part for slot requests
slot_timeout = 60												;timeout if not used in seconds
slot_db = "/var/lib/prosody/http_upload/slot_db"				;save currently active slots here (used by the http part)
put_prefix = "https://proxy.example.org:5281/upload/"			;url prefix for put requests
get_prefix = "https://proxy.example.org:5281/upload/"			;url prefix for get requests
component_host = "proxy.example.org"							;component hostname
component_password = "1234"										;component password for xmpp server
xmpp_host = "localhost"											;xmpp server address
xmpp_port = 5347												;xmpp server component port
log_level = 'INFO'												;DEBUG, INFO, NOTICE, WARNING and ERROR
keepalive = 30													;send keep alive whitespaces every 30 seconds